{
  "CookieDomain": ".localhost",
  "FrontendURL": "http://localhost:5000",
  "Logging": {
    "Console": {
      "MinimumLevel": "Debug"
    },
    "Graylog": {
      "MinimumLevel": "Debug"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "SwarmBotSwaggerRoute": {
        "ClusterId": "SwarmBotCluster",
        "Match": {
          "Path": "swarm-bot/swagger/v1/swarm-bot/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      },
      "MembersSwaggerRoute": {
        "ClusterId": "MembersCluster",
        "Match": {
          "Path": "members-ms/swagger/v1/members-ms/{**catch-all}"
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      }
    }
  }
}
